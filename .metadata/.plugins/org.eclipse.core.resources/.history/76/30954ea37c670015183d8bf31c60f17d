	// September 30 2015
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.println("Enter 1 character:");
		String c1 = scan.next();
		System.out.println("Enter another character:");
		String c2= scan.next();
		System.out.println("Enter another character:");		
		String c3 = scan.next();
		System.out.println("Enter another character:");
		String c4 = scan.next();
		System.out.println("Enter another character:");
		String c5 = scan.next();
		scan.close();
		char a1 = c1.charAt(0);
		char a2 = c2.charAt(0);
		char a3 = c3.charAt(0);
		char a4 = c4.charAt(0);
		char a5 = c5.charAt(0);
		int b1 =  a1;
		int b2 =  a1;
		int b3 =  a1;
		int b4 =  a1;
		int b5 =  a1;
		int total = b1 + b2 + b3 + b4 + b5;
		System.out.println(a1 + " "+ a2 + " " + a3 + " "+ a4 + " "+ a5 + " has a value of " + total);
		System.out.println(a5 + " has an ASCII value of " + (int) a5);

	}

}
